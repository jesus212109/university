# Test incident edges in current vertex.
GRAPH_CREATE 1
ADD_VERTEX A
ADD_VERTEX B
ADD_VERTEX C
ADD_EDGE 0 1 0.5
ADD_EDGE 1 0 0.25
ADD_EDGE 1 2 0.7
ADD_EDGE 2 1 0.75
GOTO_VERTEX 0
PRINT_CURRENT_VERTEX
PRINT_INCIDENT_EDGES
GOTO_VERTEX 1
PRINT_CURRENT_VERTEX
PRINT_INCIDENT_EDGES
GOTO_VERTEX 2
PRINT_CURRENT_VERTEX
PRINT_INCIDENT_EDGES
