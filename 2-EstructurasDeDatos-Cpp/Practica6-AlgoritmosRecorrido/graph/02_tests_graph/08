# Test goto edge in undirected graph
GRAPH_CREATE 0
ADD_VERTEX A
ADD_VERTEX B
ADD_VERTEX C
ADD_EDGE 0 1 0.5
ADD_EDGE 1 2 0.7
GOTO_EDGE 0 1
PRINT_CURRENT_VERTEX
PRINT_CURRENT_EDGE
GOTO_EDGE 1 0
PRINT_CURRENT_VERTEX
PRINT_CURRENT_EDGE
GOTO_EDGE 1 2
PRINT_CURRENT_VERTEX
PRINT_CURRENT_EDGE
GOTO_EDGE 2 1
PRINT_CURRENT_VERTEX
PRINT_CURRENT_EDGE
GOTO_EDGE2 0
GOTO_EDGE2 1
