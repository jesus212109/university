  "version": "0.2.0",
  "configurations": [
    {
      "name": "test_alphabet_soup",
      "type": "cppdbg",
      "request": "launch",
      "program": "/tmp/build-alphabet_soup-Debug/test_alphabet_soup_solver",
      "args": ["tests_alphabet_soup/01"],
      "stopAtEntry": false,
      "cwd": "${fileWorkspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
